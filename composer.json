{
    "name": "edalzell/download",
    "license": "MIT",
    "description": "Allow download of private assets by logged in users",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.61",
        "statamic/cms": "^3.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Edalzell\\Download\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Download",
            "description": "Download addon"
        },
        "laravel": {
            "providers": [
                "Edalzell\\Download\\ServiceProvider"
            ]
        }
    }
}
